<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D8E7FD"
    android:fitsSystemWindows="true">


        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="706dp"
            android:fontFamily="@font/gilroyextra"
            android:layout_marginTop="30dp"
            android:textSize="15sp"
            android:textAlignment="center"
            android:layout_marginBottom="23dp"
            android:text="@string/selected_item1"

            app:layout_anchor="@+id/textView"
            app:layout_anchorGravity="bottom|center" />

        <androidx.core.widget.NestedScrollView
            android:id="@+id/nestedScrollView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true"
            android:scrollbars="vertical"
            app:layout_anchor="@+id/nestedScrollView"
            app:layout_anchorGravity="center"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                        <Button
                            android:id="@+id/imagechicken"
                            android:layout_width="156dp"
                            android:layout_height="0dp"
                            android:layout_marginBottom="10dp"
                            android:background="@drawable/chicken"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagechicken1"
                            android:text="CHICKEN"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFFFF"
                            android:textSize="25dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/imagepasta" />

                        <Button
                            android:id="@+id/imagepasta"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_alignEnd="@+id/imagemiscellaneous"
                            android:layout_marginStart="32dp"
                            android:layout_marginTop="85dp"
                            android:layout_marginEnd="27dp"
                            android:layout_marginBottom="916dp"
                            android:background="@drawable/pasta"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagepasta1"
                            android:text="PASTA"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFAFA"
                            android:textSize="25dp"
                            app:layout_constraintBottom_toTopOf="@+id/imagechicken"
                            app:layout_constraintEnd_toStartOf="@+id/imagevegetarian"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                        <Button
                            android:id="@+id/imagebeef"
                            android:layout_width="0dp"
                            android:layout_height="124dp"
                            android:layout_marginStart="33dp"
                            android:layout_marginTop="172dp"
                            android:layout_marginEnd="27dp"
                            android:layout_marginBottom="172dp"
                            android:background="@drawable/beef"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagebeef1"
                            android:text="BEEF"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFFFF"
                            android:textSize="25dp"
                            app:layout_constraintBottom_toTopOf="@+id/imagebreakfast"
                            app:layout_constraintEnd_toStartOf="@+id/imageseafood"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/imagevegetarian" />

                        <Button
                            android:id="@+id/imagedessert"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginEnd="37dp"
                            android:layout_marginBottom="24dp"
                            android:background="@drawable/dessert"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagedessert1"
                            android:text="DESSERT"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFFFF"
                            android:textSize="25dp"
                            app:layout_constraintBottom_toTopOf="@+id/imagestarter"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imagegoat"
                            app:layout_constraintTop_toBottomOf="@+id/imageseafood" />

                        <Button
                            android:id="@+id/imagelamb"
                            android:layout_width="0dp"
                            android:layout_height="124dp"
                            android:layout_marginStart="32dp"
                            android:layout_marginEnd="28dp"
                            android:background="@drawable/lamb"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagelamb1"
                            android:text="LAMB"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFFFF"
                            android:textSize="25dp"
                            app:layout_constraintBaseline_toBaselineOf="@+id/imageside"
                            app:layout_constraintEnd_toStartOf="@+id/imageside"
                            app:layout_constraintStart_toStartOf="parent" />

                        <Button
                            android:id="@+id/imagemiscellaneous"
                            android:layout_width="0dp"
                            android:layout_height="125dp"
                            android:layout_marginEnd="44dp"
                            android:background="@drawable/miscallenous"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagemiscellaneous1"
                            android:text="MISCELLANEOUS"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFFFF"
                            android:textSize="19dp"
                            app:layout_constraintBaseline_toBaselineOf="@+id/imagefastfood"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imagefastfood" />

                        <Button
                            android:id="@+id/imagepork"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginStart="33dp"
                            android:layout_marginEnd="24dp"
                            android:layout_marginBottom="113dp"
                            android:background="@drawable/pork"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagepork1"
                            android:text="  PORK"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFFFF"
                            android:textSize="25dp"
                            app:layout_constraintBottom_toTopOf="@+id/back5"
                            app:layout_constraintEnd_toStartOf="@+id/imagevegan"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/imagefastfood" />

                        <Button
                            android:id="@+id/imageseafood"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginEnd="39dp"
                            android:layout_marginBottom="23dp"
                            android:background="@drawable/seafood"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imageseafood1"
                            android:text="Sea food"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFFFF"
                            android:textSize="25dp"
                            app:layout_constraintBottom_toTopOf="@+id/imagedessert"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imagebeef"
                            app:layout_constraintTop_toBottomOf="@+id/imageside" />

                        <Button
                            android:id="@+id/imageside"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginEnd="37dp"
                            android:layout_marginBottom="26dp"
                            android:background="@drawable/side"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imageside1"
                            android:text="SIDE"
                            android:textAlignment="center"
                            android:textColor="#FFFFFF"
                            android:textSize="27dp"
                            app:layout_constraintBottom_toTopOf="@+id/imageseafood"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imagelamb"
                            app:layout_constraintTop_toBottomOf="@+id/imagevegetarian" />

                        <Button
                            android:id="@+id/imagefastfood"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginStart="33dp"
                            android:layout_marginEnd="22dp"
                            android:layout_marginBottom="22dp"
                            android:background="@drawable/fastfood"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagefastfood1"
                            android:text="FAst food"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFFFF"
                            android:textSize="25dp"
                            app:layout_constraintBottom_toTopOf="@+id/imagepork"
                            app:layout_constraintEnd_toStartOf="@+id/imagemiscellaneous"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/imagestarter" />

                        <Button
                            android:id="@+id/imagestarter"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginEnd="38dp"
                            android:layout_marginBottom="24dp"
                            android:background="@drawable/starter"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagestarter1"
                            android:text="  Starter"
                            android:textAlignment="viewStart"
                            android:textColor="#FDECF2"
                            android:textSize="25dp"
                            app:layout_constraintBottom_toTopOf="@+id/imagefastfood"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imagebreakfast"
                            app:layout_constraintTop_toBottomOf="@+id/imagedessert" />

                        <Button
                            android:id="@+id/imagevegan"
                            android:layout_width="0dp"
                            android:layout_height="124dp"
                            android:layout_marginEnd="44dp"
                            android:background="@drawable/vegan"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagevegan1"
                            android:text="VEGAN"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFFFF"
                            android:textSize="25dp"
                            app:layout_constraintBaseline_toBaselineOf="@+id/imagepork"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imagepork" />

                        <Button
                            android:id="@+id/imagevegetarian"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_marginTop="85dp"
                            android:layout_marginEnd="38dp"
                            android:layout_marginBottom="20dp"
                            android:background="@drawable/vegetarian"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagevegetarian1"
                            android:text="VegetarÄ±an"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFFFF"
                            android:textSize="25dp"
                            app:layout_constraintBottom_toTopOf="@+id/imageside"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imagepasta"
                            app:layout_constraintTop_toTopOf="parent" />

                        <Button
                            android:id="@+id/imagebreakfast"
                            android:layout_width="0dp"
                            android:layout_height="124dp"
                            android:layout_marginStart="32dp"
                            android:layout_marginEnd="29dp"
                            android:background="@drawable/kahvalti"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagebreakfast1"
                            android:text="Breakfast"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFFFF"
                            android:textSize="25dp"
                            app:layout_constraintBaseline_toBaselineOf="@+id/imagestarter"
                            app:layout_constraintEnd_toStartOf="@+id/imagestarter"
                            app:layout_constraintStart_toStartOf="parent" />

                        <Button
                            android:id="@+id/imagegoat"
                            android:layout_width="0dp"
                            android:layout_height="124dp"
                            android:layout_marginStart="33dp"
                            android:layout_marginTop="148dp"
                            android:layout_marginEnd="29dp"
                            android:layout_marginBottom="148dp"
                            android:background="@drawable/goat"
                            android:fontFamily="@font/gilroyextra"
                            android:onClick="imagegoat1"
                            android:text="Goat"
                            android:textAlignment="viewStart"
                            android:textColor="#FFFFFF"
                            android:textSize="25dp"
                            app:layout_constraintBottom_toBottomOf="@+id/imagebreakfast"
                            app:layout_constraintEnd_toStartOf="@+id/imagedessert"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/imagebeef" />

                        <Button
                            android:id="@+id/back5"
                            android:layout_width="126dp"
                            android:layout_height="33dp"
                            android:layout_marginStart="164dp"
                            android:text="BACK"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/imagechicken"
                            app:layout_constraintTop_toBottomOf="@+id/imagepork"
                            app:layout_constraintVertical_bias="1.0" />

                </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.core.widget.NestedScrollView>

</androidx.coordinatorlayout.widget.CoordinatorLayout>